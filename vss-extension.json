{
  "manifestVersion": 1,
  "id": "devops-heroku-tools",
  "name": "Azure DevOps Heroku Tools",
  "version": "0.1.0",
  "publisher": "ckrnstck",
  "public": false,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Azure Pipeline tasks for Heroku deployment.",
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "vss-extension-icon.png"
  },
  "files": [
    {
      "path": "Tasks/PushToHeroku"
    }
  ],
  "content": {
    "details": {
      "path": "vss-extension-details.md"
    }
  },
  "contributions": [
    {
      "id": "pushu-to-heroku",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks/PushToHeroku"
      }
    }
  ],
  "links": {
    "home": {
      "uri": "https://github.com/prc322/azure-devops-heroku-tools"
    },
    "getstarted": {
      "uri": "https://github.com/prc322/azure-devops-heroku-tools"
    },
    "issues": {
      "uri": "https://github.com/prc322/azure-devops-heroku-tools"
    },
    "license": {
      "uri": "https://github.com/prc322/azure-devops-heroku-tools"
    },
    "repository": {
      "uri": "https://github.com/prc322/azure-devops-heroku-tools"
    }
  }
}